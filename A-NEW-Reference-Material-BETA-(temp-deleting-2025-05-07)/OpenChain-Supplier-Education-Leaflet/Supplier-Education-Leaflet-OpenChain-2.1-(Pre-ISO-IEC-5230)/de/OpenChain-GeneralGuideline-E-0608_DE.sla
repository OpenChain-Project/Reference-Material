        <PAGEOBJECT XPOS="139.685039370079" YPOS="1129.54685039248" OwnPage="1" ItemID="739928321" PTYPE="4" WIDTH="351.246062992005" HEIGHT="19.8425196850394" FRTYPE="3" CLIPEDIT="1" PWIDTH="0" PLINEART="1" ANNAME="u1de2" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="12" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L0 19.8425 L351.246 19.8425 L351.246 0 L0 0 Z" copath="M0 0 L0 19.8425 L351.246 19.8425 L351.246 0 L0 0 Z" fillRule="0" gXpos="139.685039370079" gYpos="1129.54685039248" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
